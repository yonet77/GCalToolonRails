
<%= link_to 'CSV', :action=>:export, :data=>@lists, :method=>:post %>
<br><br>
<% @lists.each_with_index do |e, idx| %>
  (#<%= idx %>) タイトル：<%= e[:title] %><br>
  説明：<%= e[:desc] %><br>
  期間：<%= e[:st].localtime.strftime("%Y/%m/%d %H:%M") %> ～ <%= e[:en].localtime.strftime("%Y/%m/%d %H:%M") %><br>
  時間：<%= (e[:en]-e[:st])/(60*60) %>
  <hr>
<% end %>
